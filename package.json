  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./lib && babel ./src --out-dir ./lib && yarn run build-styles",
    "build-styles": "find src/ -name '*.less' ! -name '*import.less' -exec lessc {} \\; > ./lib/styles.css",
    "prepublish": "yarn run build",
    "test": "yarn run lint && jest",
    "watch": "node watch.js",
    "test-watch": "jest --watch",
    "lint": "standard \"src/**/*.js\""
  },
  "devDependencies": {
    "apollo-client": "^2.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "colors": "^1.3.3",
    "eslint-config-orionsoft": "^1.3.12",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "jest-cli": "^24.5.0",
    "less": "^3.9.0",
    "react": "^16.8.4",
    "react-apollo": "^2.5.2",
    "react-dom": "^16.8.6",
    "simple-react-form": "^2.2.1"
  },
  "peerDependencies": {
    "apollo-client": "^2.4.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.9.2",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "simple-react-form": "^2.1.3"
  },
  "dependencies": {
    "@orion-js/schema": "^0.12.0",
    "autobind-decorator": "^2.4.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2"
  }
}
